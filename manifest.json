{
  "name": "k8s nodes health-checker",
  "description": "Monitor kubelet's health",
  "version": "1.0",
  "background": {
     "scripts": ["background.js"],
     "persistent": false
   },
  "permissions": [
    "tabs",
    "alarms",
    "storage",
    "http://*/*"
  ],
  "browser_action": {
      "default_icon" : "icon.png",
      "name": "k8s"
  },
  "manifest_version": 2
}
